<div class="row">
    <div class="box box-primary">
        <div class="box-header col s12">
            <h2 class="col s12 box-title left-align show">
                {{ 'label.julgamentoProposta.participante'|trans }}
            </h2>
            <div class="divider grey title-divider"></div>
        </div>
        <div class="boxy-body">
            <div class="col s12">
                <table class="table show-table">
                    <tbody>
                    <tr class="sonata-ba-view-container">
                        <td>{{ 'label.julgamentoProposta.participante'|trans }}</td>
                        <td>{{ object.participante.fkSwCgm }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <td>{{ 'label.julgamentoProposta.tipoFornecedor'|trans }}</td>
                        <td>{{ object.participante.tipo }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <td>{{ 'label.julgamentoProposta.item'|trans }}</td>
                        <td>{{ object.cotacaoItem.fkAlmoxarifadoCatalogoItem.descricaoResumida }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <td>{{ 'label.julgamentoProposta.lote'|trans }}</td>
                        <td>{{ object.cotacaoItem.lote }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <td>{{ 'label.julgamentoProposta.quantidade'|trans }}</td>
                        <td>{{ object.cotacaoItem.quantidade|number_format(4, ',', '.') }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <td>{{ 'label.julgamentoProposta.valorUnitario'|trans }}</td>
                        <td>R$ {{ (object.cotacaoFornecedorItem.vlCotacao/object.cotacaoItem.quantidade)|number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <td>{{ 'label.julgamentoProposta.valorTotal'|trans }}</td>
                        <td>R$ {{ object.cotacaoFornecedorItem.vlCotacao|number_format(2, ',', '.') }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <td>{{ 'label.julgamentoProposta.classificacao'|trans }}</td>
                        <td>{{ object.classificacao }}ยบ</td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
