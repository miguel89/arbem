{% if admin.request.requestUri matches "{\/(delete)}" == false and admin.request.requestUri matches "{\/(list)}" == false and admin.request.requestUri matches "{\/(show)}" == false %}
<div class="row">
    <div class="box box-primary">
        <div class="box-header col s12">
            <h2 class="col s12 box-title left-align show">
                {{ 'label.manutencaoProposta.manutencaoProposta'|trans }}
            </h2>
            <div class="divider grey title-divider"></div>
        </div>
        <div class="box-body">
            <div class="col s12">
                <table class="table show-table">
                    <tbody>
                    <tr class="sonata-ba-view-container">
                        <th>{{ 'label.manutencaoProposta.codEntidade'|trans }}</th>
                        <td>{{ admin.compraDireta.fkOrcamentoEntidade.codEntidade ~ " - " ~ admin.compraDireta.fkOrcamentoEntidade.fkSwCgm.nomCgm }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <th>{{ 'label.manutencaoProposta.codMapa'|trans }}</th>
                        <td>{{ admin.compraDireta.fkComprasMapa.codMapa ~ '/' ~ admin.compraDireta.fkComprasMapa.exercicio }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <th>{{ 'label.manutencaoProposta.codCompraDireta'|trans }}</th>
                        <td>{{ admin.compraDireta.codCompraDireta }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <th>{{ 'label.manutencaoProposta.objeto'|trans }}</th>
                        <td>{{ admin.compraDireta.fkComprasObjeto.codObjeto ~ ' - ' ~ admin.compraDireta.fkComprasObjeto.descricao }}</td>
                    </tr>
                    <tr class="sonata-ba-view-container">
                        <th>{{ 'label.manutencaoProposta.codModalidade'|trans }}</th>
                        <td>{{ admin.compraDireta.fkComprasModalidade.codModalidade ~ ' - ' ~ admin.compraDireta.fkComprasModalidade.descricao }}</td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
{% endif %}
