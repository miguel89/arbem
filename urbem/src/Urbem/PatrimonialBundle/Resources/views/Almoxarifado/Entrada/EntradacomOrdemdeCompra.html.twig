{% block content%}
<div class="col-lg-12 box-title left-align show">
    <h3>Itens a serem atendidos</h3>
</div>
<div class="divider grey lighten-1  title-divider"><p style="width: 17%; float: left">1</p></div>
<div style="text-align: left; padding-top: 20px">
    <div class="panel-group" id="accordion">
        <div class="panel panel-default">
            <div class="panel-heading" style="height: 45px; background-color: #ccc;">
                <h4 class="panel-title">
                    <a data-toggle="collapse" data-parent="#accordion" href="#collapse1">
                        <p style="width: 5%; float: left; font-size: 16px">
                            <i class="material-icons">library_add</i>
                        </p>
                        <p style="width: 5%; float: left">1</p>
                        <p style="width: 20%; float: left">
                            <span style="font-weight: bold">Item:</span><br/> Codigo e Descrição</p>
                        <p style="width: 20%; float: left">
                            <span style="font-weight: bold">Unidade de Medida:</span><br/> Unidade</p>
                        <p style="width: 20%; float: left">
                            <span style="font-weight: bold">Centro de Custo:</span><br/> Codigo e Descrição</p>
                        <p style="width: 15%; float: left">
                            <span style="font-weight: bold">Solicitado OC:</span><br/> xyz </p>
                        <p style="width: 15%; float: left">
                            <span style="font-weight: bold">Atendimento OC:</span><br/> xyz </p>
                    </a>
                </h4>
            </div>
            <div id="collapse1" class="panel-collapse collapse">
                <div class="panel-body">
                    <a href="#" style="width: 5%; float: right; font-size: 16px">
                        <i class="material-icons">edit</i>
                    </a>
                    <p>
                        <span style="font-weight: bold">
                            Código do Item:
                        </span>
                        Codigo
                    </p>
                    <p>
                        <span style="font-weight: bold">
                            Descrição:
                        </span>
                        Descrição
                    </p>
                    <p>
                        <span style="font-weight: bold">
                            Grandeza:
                        </span>
                        Unidade
                    </p>
                    <p>
                        <span style="font-weight: bold">
                            Unidade:
                        </span>
                        Unidade
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}