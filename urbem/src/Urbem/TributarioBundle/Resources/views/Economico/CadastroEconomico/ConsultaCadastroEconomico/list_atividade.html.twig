{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}
{% block field%}
	{% set atividade = '' %}
	{% for atividadeCadastroEconomico in object.getFkEconomicoAtividadeCadastroEconomicos() %}
		{% if loop.index == 0 %}
			{% set atividade =  atividadeCadastroEconomico.getFkEconomicoAtividade() %}
		{% endif %}

		{% if atividadeCadastroEconomico.getPrincipal() %}
			{% set atividade =  atividadeCadastroEconomico.getFkEconomicoAtividade() %}
		{% endif %}
	{% endfor %}

	{{ atividade }}
{% endblock %}
