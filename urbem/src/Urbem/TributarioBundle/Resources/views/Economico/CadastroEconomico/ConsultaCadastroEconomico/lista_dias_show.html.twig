<table class="table show-table">
    <thead>
        <tr>
            <th>{{ 'label.economicoAtividadeCadastroEconomico.diaSemana'|trans }}</th>
            <th>{{ 'label.economicoAtividadeCadastroEconomico.hrInicio'|trans }}</th>
            <th>{{ 'label.economicoAtividadeCadastroEconomico.hrTermino'|trans }}</th>
        </tr>
    </thead>
    <tbody>
        {% for item in admin.cadastroEconomico.getFkEconomicoDiasCadastroEconomicos() %}
            <tr class="sonata-ba-view-container">
                <td>
                    <span>{{ item.getFkAdministracaoDiasSemana().getNomDia() }}</span>
                </td>
                <td>
                    <span>{{ item.getHrInicio().format('H:i') }}</span>
                </td>
                <td>
                    <span>{{ item.getHrTermino().format('H:i') }}</span>
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
