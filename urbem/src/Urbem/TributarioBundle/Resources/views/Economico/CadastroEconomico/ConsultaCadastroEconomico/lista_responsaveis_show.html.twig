<table class="table show-table">
    <thead>
        <tr>
            <th>{{ 'label.economicoCadastroEconRespTecnico.numcgm'|trans }}</th>
            <th>{{ 'label.economicoCadastroEconRespTecnico.nomCgm'|trans }}</th>
            <th>{{ 'label.economicoCadastroEconRespTecnico.numRegistro'|trans }}</th>
            <th>{{ 'label.economicoCadastroEconRespTecnico.nomProfissao'|trans }}</th>
        </tr>
    </thead>
    <tbody>
        {% for item in admin.responsaveisTecnicos %}
            <tr class="sonata-ba-view-container">
                <td>
                    <span>{{ item.getNumcgm() }}</span>
                </td>
                <td>
                    <span>{{ item.getFkSwCgm().getNomCgm() }}</span>
                </td>
                <td>
                    <span>{{ "%s - %s"|format(item.getNumRegistro(), item.getFkSwUf().getSiglaUf()) }}</span>
                </td>
                <td>
                    <span>{{ item.getFkCseProfissao().getNomProfissao() }}</span>
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
