{% if admin.canBaixar(object) %}
    <a href="{{ admin.generateObjectUrl('baixar', object) }}" data-toggle="tooltip" data-placement="bottom" title="Baixar Elemento" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="fa fa-download fa-lg" aria-hidden="true"></i>
    </a>
{% else %}
    <a href="#" data-toggle="tooltip" data-placement="bottom" title="Elemento baixado" class="grey-text transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="fa fa-download fa-lg" aria-hidden="true"></i>
    </a>
{% endif %}