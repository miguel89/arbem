{% if admin.verificaBaixaNaturezaJuridica(object) %}
    <a href="#" data-toggle="tooltip" data-placement="bottom" title="Elemento baixado" class="grey-text transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="fa fa-download fa-lg" aria-hidden="true"></i>
    </a>
{% else %}
    <a href="{{ admin.generateObjectUrl('baixar', object) }}" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped" data-toggle="tooltip" data-placement="bottom" title="Baixar Natureza">
        <i class="fa fa-download fa-lg" aria-hidden="true"></i>
    </a>
{% endif %}