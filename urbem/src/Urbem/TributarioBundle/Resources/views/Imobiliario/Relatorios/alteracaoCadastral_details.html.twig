        {% if filtros['loteFinal'] %}
            <p><strong>{{ 'label.imobiliarioRelatorios.alteracaoCadastral.loteAte'|trans }} :</strong>  {{ filtros['loteFinal'] }}</p>
        {% endif %}

        {% if filtros['inscricaoImobiliariaInicial'] %}
