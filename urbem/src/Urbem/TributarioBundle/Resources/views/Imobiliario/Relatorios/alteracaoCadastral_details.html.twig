        {% if filtros['atributos'] %}
            <p>
                <strong>{{ 'label.imobiliarioRelatorios.alteracaoCadastral.atributosRelacionados'|trans }} :</strong>
                {% for atributo in filtros['atributos'] %}
                    <br>{{ (admin.getNomeAtributo(atributo)).getNomAtributo()|capitalize }}
                {% endfor %}
            </p>
        {% endif %}

        <p>
            <strong>{{ 'label.imobiliarioRelatorios.alteracaoCadastral.ordenacao'|trans }} :</strong> {{ (admin.getOrdenacao()[filtros['ordenacao']])|trans }}
