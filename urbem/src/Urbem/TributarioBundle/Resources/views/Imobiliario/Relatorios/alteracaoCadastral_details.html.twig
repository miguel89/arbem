        {% endif %}

        {% if filtros['codigoBairroInicial'] %}
            <p><strong>{{ 'label.imobiliarioRelatorios.alteracaoCadastral.codigoBairroDe'|trans }} :</strong>  {{ filtros['codigoBairroInicial'] }}</p>
        {% endif %}

        {% if filtros['codigoBairroFinal'] %}
            <p><strong>{{ 'label.imobiliarioRelatorios.alteracaoCadastral.codigoBairroAte'|trans }} :</strong>  {{ filtros['codigoBairroFinal'] }}</p>
        {% endif %}

