{% extends "base_home_cards.html.twig" %}
{% block content %}
    <div class="container">
        <div class="container">
            <h4 class="blue-text text-darken-4"><i class="mdi mdi-bank left"></i>{{ 'label.configuracaoArrecadacao.modulo'|trans }}</h4>
            <div class="divider grey title-divider"><p>1</p></div>
        </div>
        <div class="container">
            <div class="col s6 m6 l4">
                <div class="card-panel col s12 hoverable center">
                    <a href="{{ path('urbem_administrativo_administracao_configuracao_create', {id : 25}) }}">
                        <h6 class="truncate center">{{ 'label.configuracaoArrecadacao.subModulo'|trans }}</h6>
                    </a>
                    <p></p>
                    <div class="divider col s12"></div>
                    <h5 class="left">
                        <a href="{{ path('urbem_administrativo_administracao_configuracao_create', {id : 25}) }}" class="tooltipped" data-position="bottom" data-delay="50" data-tooltip="Editar"><i class="material-icons">edit</i></a>
                    </h5>
                </div>
            </div>

            <div class="col s6 m6 l4">
                <div class="card-panel col s12 hoverable center">
                    <a href="{{ path('urbem_administrativo_administracao_configuracao_create', {id : 25, tipo: 'credito'}) }}">
                        <h6 class="truncate center">{{ 'label.configuracaoGrupoCreditos.subModulo'|trans }}</h6>
                    </a>
                    <p></p>
                    <div class="divider col s12"></div>
                    <h5 class="left">
                        <a href="{{ path('urbem_administrativo_administracao_configuracao_create', {id : 25, tipo: 'credito'}) }}" class="tooltipped" data-position="bottom" data-delay="50" data-tooltip="Editar"><i class="material-icons">edit</i></a>
                    </h5>
                </div>
            </div>
        </div>
    </div>
{% endblock content %}
