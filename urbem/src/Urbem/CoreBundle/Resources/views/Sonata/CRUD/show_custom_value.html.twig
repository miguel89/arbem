{% extends 'CoreBundle:Sonata/CRUD:base_show_field.html.twig' %}

{% block field %}
    {% if value is empty %}
        {% if field_description.options.data is defined and field_description.options.data is not empty %}
            {{ field_description.options.data }}
        {% else %}
            -
        {% endif %}
    {% else %}
        {{ value }}
    {% endif %}
{% endblock field %}
