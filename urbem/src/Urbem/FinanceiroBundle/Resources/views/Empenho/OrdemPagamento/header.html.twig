<table class="table table-bordered">
    <tbody>
    <tr>
        <td rowspan="5" style="border-bottom: 0; border-left: 0; border-top: 0">
            <img class="responsive-img" style="height: 65px; margin-top: 20px"
                 src="http://urbem.cnm.org.br/bundles/portal/images/logo_urbem.png">
        </td>
        <th rowspan="5" colspan="6">
            {{ object.fkOrcamentoEntidade.fkSwCgm.nomCgm }}<br />
            {{ 'foneFax'|trans }}: {{ object.fkOrcamentoEntidade.fkSwCgm.foneResidencial }} / {{ object.fkOrcamentoEntidade.fkSwCgm.foneComercial }}<br />
            {{ 'email'|trans }}: {{ object.fkOrcamentoEntidade.fkSwCgm.eMail }}<br />
            {{ object.fkOrcamentoEntidade.fkSwCgm.tipoLogradouro }}
            {{ object.fkOrcamentoEntidade.fkSwCgm.logradouro }}, {{ object.fkOrcamentoEntidade.fkSwCgm.numero }} - {{ object.fkOrcamentoEntidade.fkSwCgm.bairro }}<br />
            {{ 'cep'|trans }}: {{ object.fkOrcamentoEntidade.fkSwCgm.cep|slice(0,5) }}-{{ object.fkOrcamentoEntidade.fkSwCgm.cep|slice(5) }}<br />
            {{ 'cnpj'|trans }}: {{ object.fkOrcamentoEntidade.fkSwCgm.fkSwCgmPessoaJuridica.cnpj|slice(0,2) }}.{{ object.fkOrcamentoEntidade.fkSwCgm.fkSwCgmPessoaJuridica.cnpj|slice(2,3) }}.{{ object.fkOrcamentoEntidade.fkSwCgm.fkSwCgmPessoaJuridica.cnpj|slice(5,3) }}/{{ object.fkOrcamentoEntidade.fkSwCgm.fkSwCgmPessoaJuridica.cnpj|slice(8,4) }}-{{ object.fkOrcamentoEntidade.fkSwCgm.fkSwCgmPessoaJuridica.cnpj|slice(12) }}
        </th>
        <td>{{ modulo|trans }}</td>
        <td>
            <span class="bold">{{ 'versao'|trans }}</span>
            {{ versao }}
        </td>

    </tr>
    <tr>
        <td>{{ subModulo|trans }}</td>
        <td>
            <span class="bold">{{ 'usuario'|trans }}</span>
            {{ usuario.username }}
        </td>
    </tr>
    <tr>
        <td>{{ funcao|trans }}</td>
        <td>{{ nomRelatorio|trans({'%numOrdem%': object.codOrdem ~ '/' ~ object.exercicio, '%dtVencimento%': object.dtVencimento|date('d/m/Y')}) }}</td>
    </tr>
    <tr>
        <td colspan="2">
            <span class="bold">{{ 'emissao'|trans }}</span>
            {{ dtEmissao|date('d/m/Y') }}
        </td>
    </tr>
    </tbody>
</table>
<div class="divider">
    <p></p>
</div>