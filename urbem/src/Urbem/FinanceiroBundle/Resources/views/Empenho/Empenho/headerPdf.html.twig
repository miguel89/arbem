<table class="table table-bordered">
    <tbody>
    <tr>
        <td rowspan="5" style="border-bottom: 0; border-left: 0; border-top: 0">
            <img class="responsive-img" style="height: 65px; margin-top: 30px"
                 src="http://urbem.cnm.org.br/bundles/portal/images/logo_urbem.png">
        </td>
        <th rowspan="5" colspan="6">
            {{ dadosEmpenho.nomEntidade }}<br />
            Fone/Fax: {{ dadosEmpenho.foneEntidade }}<br />
            E-mail: {{ dadosEmpenho.emailEntidade }}<br />
            {{ dadosEmpenho.logradouro }}<br />
            CEP: {{ dadosEmpenho.cepEntidade }}<br />
            CNPJ: {{ dadosEmpenho.cnpjEntidade }}
        </th>
        <td>{{ modulo }}</td>
        <td>
            <span class="bold">Versão:</span>
            {{ versao }}
        </td>
    </tr>
    <tr>
        <td>{{ subModulo }}</td>
        <td>
            <span class="bold">Usuario:</span>
            {{ usuario.username }}
        </td>
    </tr>
    <tr>
        <td>{{ funcao }}</td>
        <td>{{ nomRelatorio }}</td>
    </tr>
    <tr>
        <td>
            <span class="bold">Emissão:</span>
            {{ emissaoDocumento.dtEmpenho|date('d/m/Y') ~" "~  emissaoDocumento.hora|date("H:i:s") }}
        </td>
        <td>
            <span class="bold">Hora:</span>
            {{ emissaoDocumento.hora|date("H:i:s") }}
        </td>
    </tr>
    </tbody>
</table>
<div class="divider">
    <p></p>
</div>