{% if object.fkTesourariaReciboExtraAnulacao is null %}
    <a href="{{ path('urbem_financeiro_tesouraria_recibo_extra_relatorio', { 'codReciboExtra': object.codReciboExtra,'exercicio': object.exercicio, 'codEntidade': object.codEntidade, 'tipoRecibo': object.tipoRecibo }) }}"
       class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped  btn-list"
       data-toggle="tooltip"
       data-placement="bottom"
       target="_blank"
       title="{{ 'label.reciboExtra.emitirRecibo'|trans }}">
        <i class="fa fa-print fa-lg" aria-hidden="true"></i>
    </a>
{% else %}
    <a href="#"
       class="grey-text z-depth-0 hide-on-med-and-down tooltipped  btn-list"
       data-toggle="tooltip"
       data-placement="bottom"
       title="Anulado">
        <i class="fa fa-print fa-lg grey-text" aria-hidden="true"></i>
    </a>
{% endif %}