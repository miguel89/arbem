{% if admin.isGranted('DELETE', object) and admin.hasRoute('delete') %}
    {% if object.fkTesourariaReciboExtraAnulacao is null %}
        <a href="{{ admin.generateObjectUrl('delete', object) }}" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list" data-toggle="tooltip" data-placement="bottom" title="Anular">
            <i class="fa fa-ban fa-lg" aria-hidden="true"></i>
        </a>
    {% else %}
        <a href="#" class="grey-text transparent z-depth-0 hide-on-med-and-down tooltipped btn-list" data-toggle="tooltip" data-placement="bottom" title="Anulado">
            <i class="fa fa-ban fa-lg grey-text" aria-hidden="true"></i>
        </a>
    {% endif %}
{% endif %}