{% if admin.getConciliacao(object.codPlano, admin.getFilter().exercicio, admin.getFilter().mes) is not null %}
    <a href="{{ path('urbem_financeiro_tesouraria_conciliacao_relatorio', { 'id': object.codPlano ~ '~' ~ admin.getFilter().exercicio ~ '~' ~ admin.getFilter().mes ~ '~' ~ admin.getFilter().dtExtrato }) }}"
       class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped  btn-list"
       data-toggle="tooltip"
       data-placement="bottom"
       target="_blank"
       title="{{ 'label.conciliarConta.emitirRelatorio'|trans }}">
        <i class="fa fa-print fa-lg" aria-hidden="true"></i>
    </a>
{% else %}
    <a href="#"
       class="grey-text z-depth-0 hide-on-med-and-down tooltipped  btn-list"
       data-toggle="tooltip"
       data-placement="bottom"
       title="{{ 'label.conciliarConta.emitirRelatorio'|trans }}">
        <i class="fa fa-print fa-lg grey-text" aria-hidden="true"></i>
    </a>
{% endif %}
