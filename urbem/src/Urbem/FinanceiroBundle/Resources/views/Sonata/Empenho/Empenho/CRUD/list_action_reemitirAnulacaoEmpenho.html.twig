{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') %}

    {% if admin.verificaEmpenhoAnulado(object) == false %}
        <a href="{{ path('urbem_financeiro_empenho_anular_empenho_reemitir_anulacao_empenho', { 'codPreEmpenho': object.codPreEmpenho, 'exercicio': object.exercicio }) }}">
            <i class="fa fa-file-pdf-o fa-lg" aria-hidden="true"></i>
        </a>
    {% else %}
        <i class="fa fa-file-pdf-o fa-lg grey-text" aria-hidden="true"></i>
    {% endif %}
{% endif %}
