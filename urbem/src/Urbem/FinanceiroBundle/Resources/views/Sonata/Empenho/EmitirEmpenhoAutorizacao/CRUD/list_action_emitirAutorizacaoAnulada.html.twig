{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') %}
    {% if admin.getAutorizacaoAnulada(object) %}
        <a href="{{ path('urbem_financeiro_empenho_anular_autorizacao_reemitir_anulacao_autorizacao', { 'codPreEmpenho': object.codPreEmpenho, 'exercicio': object.exercicio }) }}" title="Reemitir Autorização Anulada" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
            <i class="fa fa-file-pdf-o fa-lg" aria-hidden="true"></i>
        </a>
    {% else %}
        <a href="#" title="Autorização não está anulada" class="grey-text transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
            <i class="fa fa-file-pdf-o fa-lg" aria-hidden="true"></i>
        </a>
    {% endif %}
{% endif %}
