<div class="row">
    <div class="col s11 servidor-table">
        <a class="btn-flat">
            <i class="mdi mdi-account-multiple left"></i>
            {{ 'label.processo.listaInteressados'|trans }}
        </a>
    </div>
    <div class="col s1">
        <a href="{{ path('urbem_administrativo_protocolo_processo_interessado_create', { 'id': admin.id(swProcesso) }) }}"
           class="btn-flat tooltipped"
           data-position="right"
           data-delay="50"
           data-tooltip="{{ 'incluir'|trans }}">
            <i class="mdi mdi-plus-circle left"></i>
        </a>
    </div>

    <table class="col s12 highlight bordered">
        <thead class="thead-rh">
        <tr><th class="th-rh">
                {{ 'label.cgm'|trans }}
            </th>
            <th class="th-rh" width="100">
                {{ 'acoes'|trans }}
            </th>
        </tr>
        </thead>
        <tbody>
        {% for swProcessoInteressado in swProcesso.fkSwProcessoInteressados %}
            <tr><td>{{ swProcessoInteressado.fkSwCgm.nomCgm|upper }}</td>
                <td class="center">
                    <a class="hide-on-med-and-down blue-text text-darken-4 tooltipped"
                       data-position="left"
                       data-delay="50"
                       data-tooltip="{{ 'deletar'|trans }}"
                       href="{{ path('urbem_administrativo_protocolo_processo_interessado_delete', { 'id': admin.id(swProcessoInteressado) }) }}">
                        <i class="material-icons">delete</i>
                    </a>
                    <a class="hide-on-med-and-down blue-text text-darken-4 tooltipped"
                       data-position="left"
                       data-delay="50"
                       data-tooltip="{{ 'editar'|trans }}"
                       href="{{ path('urbem_administrativo_protocolo_processo_interessado_edit', { 'id': admin.id(swProcessoInteressado) }) }}">
                        <i class="material-icons">edit</i>
                    </a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">NÃ£o existem interessados cadastrados neste processo.</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
