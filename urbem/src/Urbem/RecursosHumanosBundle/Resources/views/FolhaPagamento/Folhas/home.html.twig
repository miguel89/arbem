{% extends "base_home_cards.html.twig" %}
{% block content %}
    {% if app.session.flashBag.has('success') %}
        <div class="alert alert-success alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
            {% for msg in app.session.flashBag.get('success') %}
                {{ msg }}
            {% endfor %}
        </div>
    {% endif %}
    {% if app.session.flashBag.has('error') %}
        <div class="alert alert-danger alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
            {% for msg in app.session.flashBag.get('error') %}
                {{ msg }}
            {% endfor %}
        </div>
    {% endif %}
    <div class="container">
        <div class="col s5">
            <ul class="collection with-header">
                <li class="collection-header">
                    <h5><i class="mdi mdi-cash left"></i>Folha Salário</h5>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_folha_situacao_create') }}"><span>Fechar/Reabrir Folha de Salário</span><i
                                    class="mdi mdi-content-save right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_contrato_servidor_periodo_list') }}">Registrar Evento por Contrato<i class="mdi mdi-content-save right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_folhas_calculo_salario_list') }}">Cálculo
                            de Salário<i class="mdi mdi-calculator right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_consulta_ficha_financeira_salario_list') }}">Consultar
                            Ficha Financeira<i class="material-icons right small">search</i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="#"><span class="grey-text">Consultar Registros de Evento</span></a><i
                                class="material-icons right small">search</i></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_registro_evento_periodo_matricula_create') }}"><span>Registrar Lote Matrículas</span><i
                                class="mdi mdi-content-save right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_registro_evento_periodo_evento_create') }}"><span>Registrar Lote Eventos</span><i
                                class="mdi mdi-content-save right small"></i></a></div>
                </li>
            </ul>
            <ul class="collection with-header">
                <li class="collection-header">
                    <h5><i class="mdi mdi-cash-multiple left"></i>Folha 13 Salário</h5>
                </li>
                <li class="collection-item">
                    <div>
                        <a href="{{ path('urbem_recursos_humanos_folha_pagamento_folhas_conceder_decimo_list') }}">
                            Conceder 13° Salário
                            <i class="mdi mdi-content-save right small"></i>
                        </a>
                    </div>
                </li>
                <li class="collection-item">
                    <div>
                        <a href="{{ path('urbem_recursos_humanos_folha_pagamento_folhas_cancelar_decimo_list') }}">
                            Cancelar 13° salário
                            <i class="mdi mdi-content-save right small"></i>
                    </div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_folhas_calculo_decimo_list') }}">Calcular
                            13° Salário</a><i class="mdi mdi-calculator right small"></i></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_consulta_ficha_financeira_decimo_list') }}">Consultar
                            Ficha Financeira<i class="material-icons right small">search</i></a></div>
                </li>
                <li class="collection-item">
                    <div>
                        <a href="{{ path('urbem_recursos_humanos_folha_pagamento_contrato_servidor_periodo_decimo_list') }}"><span>Registrar Evento de 13° Salário por Contrato</span><i
                                    class="mdi mdi-content-save right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div><span class="grey-text">Consultar Registros de Evento de 13° Salário</span><i
                                class="material-icons right small">search</i></i></div>
                </li>
            </ul>
        </div>
        <div class="col s5 right">
            <ul class="collection with-header">
                <li class="collection-header">
                    <h5><i class="mdi mdi-library-books left"></i>Folha Complementar</h5>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_complementar_create') }}"><span>Abrir/Fechar Folha Complementar</span><i
                                    class="mdi mdi-content-save right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div>
                        <a href="{{ path('urbem_recursos_humanos_folha_pagamento_contrato_servidor_complementar_list') }}">Evento
                            por contrato na folha complementar<i class="mdi mdi-content-save right small"></i></a>
                    </div>
                </li>
                <li class="collection-item">
                    <div>
                        <a href="{{ path('urbem_recursos_humanos_folha_pagamento_folhas_calculo_complementar_list') }}">Calcular
                            Folha Complementar<i class="mdi mdi-calculator right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path("urbem_recursos_humanos_folha_pagamento_registro_evento_complementar_list") }}"><span class="">Consultar Registros de Evento de Complementar</span><i
                                class="material-icons right small">search</i></a></div>
                </li>
            </ul>

            <ul class="collection with-header">
                <li class="collection-header">
                    <h5><i class="material-icons left">beach_access</i>Folha de Ferias</h5>
                </li>
                <li class="collection-item">
                    <div>
                        <a href="{{ path('urbem_recursos_humanos_folha_pagamento_contrato_servidor_periodo_ferias_list') }}"><span>Registrar Evento de Férias por Contrato</span><i
                                    class="mdi mdi-content-save right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div>
                        <a href="{{ path('urbem_recursos_humanos_folha_pagamento_consulta_registro_evento_ferias_list') }}"><span>Consultar Registros de Evento de Férias</span><i
                                    class="mdi mdi-content-save right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_consulta_ficha_financeira_ferias_list') }}">Consultar
                            Ficha Financeira<i class="material-icons right small">search</i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_folhas_calculo_ferias_list') }}">Calcular
                            Férias</span><i class="mdi mdi-calculator right small"></i></a></div>
                </li>
            </ul>

            <ul class="collection with-header">
                <li class="collection-header">
                    <h5><i class="mdi mdi-image-broken-variant left"></i>Folha Rescisão</h5>
                </li>
                <li class="collection-item">
                    <div>
                        <a href="{{ path('urbem_recursos_humanos_folha_pagamento_contrato_servidor_periodo_rescisao_list') }}"><span>Registrar Evento de Rescisão por Contrato</span><i
                                    class="mdi mdi-content-save right small"></i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="#"><span class="grey-text">Consultar Registros de Evento de Rescisão</span><i
                                    class="material-icons right small">search</i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_consulta_ficha_financeira_rescisao_list') }}">Consultar
                            Ficha Financeira<i class="material-icons right small">search</i></a></div>
                </li>
                <li class="collection-item">
                    <div><a href="{{ path('urbem_recursos_humanos_folha_pagamento_folhas_calculo_rescisao_list') }}">Calcular
                            Rescisão<i class="mdi mdi-calculator right small"></i></a></div>
                </li>
            </ul>
        </div>
    </div>
{% endblock content %}
