{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') and admin.contratoServidorCasoCausa(object) is null %}
    <a href="{{ admin.generateObjectUrl('edit', object) }}" data-toggle="tooltip" data-placement="bottom" title="Rescindir contrato" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="mdi mdi-image-broken-variant fa-lg" aria-hidden="true"></i>
    </a>
{% endif %}
