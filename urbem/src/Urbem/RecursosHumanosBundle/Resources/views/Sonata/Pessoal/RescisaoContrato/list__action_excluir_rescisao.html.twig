{% if admin.isGranted('DELETE', object) and admin.hasRoute('delete') and admin.contratoServidorCasoCausa(object) is not null %}
    <a href="{{ admin.generateObjectUrl('delete', object) }}" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list" data-toggle="tooltip" data-placement="bottom" title="Excluir">
        <i class="fa fa-trash fa-lg" aria-hidden="true"></i>
    </a>
{% endif %}
