{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') %}
    <a href="{{ admin.generateObjectUrl('create', object) }}" data-toggle="tooltip" data-placement="bottom" title="Editar" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="fa fa-pencil fa-lg" aria-hidden="true"></i>
    </a>
{% endif %}

