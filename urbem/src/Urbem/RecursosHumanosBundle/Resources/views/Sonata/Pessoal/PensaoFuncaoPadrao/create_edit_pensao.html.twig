{% if object.fkPessoalPensoes.count() > 0 and object.fkPessoalPensoes.last.fkPessoalPensaoExcluida == null %}
    <a href="{{ admin.generateObjectUrl('edit', object) }}" data-toggle="tooltip" data-placement="bottom" title="Editar"
       class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="fa fa-pencil fa-lg" aria-hidden="true"></i>
    </a>
{% else %}
    <a href="{{ admin.generateObjectUrl('create', object) }}" data-toggle="tooltip" data-placement="bottom"
       title="Incluir" class="blue-text text-darken-4 transparent z-depth-0 hide-on-med-and-down tooltipped btn-list">
        <i class="fa fa-plus-circle" aria-hidden="true"></i>
    </a>
{% endif %}

